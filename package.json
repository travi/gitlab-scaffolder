{
  "name": "@travi/gitlab-scaffolder",
  "description": "VCS scaffolder for projects to be hosted on GitLab",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/gitlab-scaffolder",
  "bugs": "https://github.com/travi/gitlab-scaffolder/issues",
  "homepage": "https://npm.im/@travi/gitlab-scaffolder",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@form8ion/overridable-prompts": "^1.2.0",
    "@travi/cli-messages": "^1.0.3"
  },
  "devDependencies": {
    "@babel/register": "7.21.0",
    "@travi/any": "2.1.8",
    "@travi/babel-preset": "3.0.100",
    "@travi/eslint-config": "3.0.31",
    "@travi/eslint-config-mocha": "1.0.19",
    "ban-sensitive-files": "1.9.19",
    "c8": "7.14.0",
    "chai": "4.3.7",
    "commitlint-config-travi": "1.4.31",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.1",
    "rollup": "3.23.1",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "15.1.0"
  }
}
