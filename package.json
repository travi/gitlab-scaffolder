{
  "name": "@travi/gitlab-scaffolder",
  "description": "VCS scaffolder for projects to be hosted on GitLab",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/gitlab-scaffolder",
  "bugs": "https://github.com/travi/gitlab-scaffolder/issues",
  "homepage": "https://npm.im/@travi/gitlab-scaffolder",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@form8ion/overridable-prompts": "^1.2.0",
    "@travi/cli-messages": "^1.0.3"
  },
  "devDependencies": {
    "@babel/register": "7.22.15",
    "@travi/any": "3.0.1",
    "@travi/babel-preset": "3.0.113",
    "@travi/eslint-config": "5.0.2",
    "@travi/eslint-config-mocha": "1.0.21",
    "ban-sensitive-files": "1.10.1",
    "c8": "8.0.1",
    "chai": "4.3.10",
    "commitlint-config-travi": "1.4.40",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "mocha": "10.2.0",
    "npm-run-all2": "6.1.1",
    "publint": "0.2.5",
    "rimraf": "5.0.5",
    "rollup": "4.3.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "17.0.1"
  }
}
